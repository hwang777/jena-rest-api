pipeline
{
	agent any
	stages
	{
		stage("build")
		{
			steps
			{
				sh 'mvn -Dmaven.test.failure.ignore=true install'
			}
		}
		stage("test")
		{
			steps
			{
				sh 'ls -l'
			}
		}
		stage("deploy")
		{
			steps
			{
				sh 'ls -l'
			}
		}
		stage("post")
		{
			steps
			{
			always
			{
				archiveArtifacts artifacts: "target/api.war", fingerprint: true
			}
			}
		}
	}

}